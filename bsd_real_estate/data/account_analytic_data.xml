<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="seq_bsd_account_analytic" model="ir.sequence">
            <field name="name">Analytic Account</field>
            <field name="code">account.analytic.account</field>
            <field name="prefix">T/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        <record model="bsd.account.asset.stage" id="bsd_sale">
            <field name="name">Available For Sales</field>
        </record>
        <record model="bsd.account.asset.stage" id="bsd_rent">
            <field name="name">Available For Rent</field>
        </record>
        <record model="bsd.account.asset.stage" id="bsd_sold">
            <field name="name">Sold</field>
        </record>
        <record model="bsd.account.asset.stage" id="bsd_book">
            <field name="name">Booked</field>
        </record>
        <record model="bsd.account.asset.stage" id="bsd_lease">
            <field name="name">On Lease</field>
        </record>
    </data>
</odoo>