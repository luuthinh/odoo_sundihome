<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record model="ir.ui.view" id="bsd_contract_sale_form">
        <field name="name">bsd.contract.sale.form.inherit</field>
        <field name="model">bsd.contract.sale</field>
        <field name="inherit_id" ref="bsd_real_estate.bsd_contract_sale_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='bsd_block_id']" position="after">
                <field name="bsd_pricelist_id"/>
            </xpath>
        </field>
    </record>

    <record model="ir.ui.view" id="bsd_account_analytic_account_form">
        <field name="name">bsd.account.analytic.form.inherit</field>
        <field name="model">account.analytic.account</field>
        <field name="inherit_id" ref="bsd_real_estate.bsd_account_analytic_account_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='currency_id']" position="after">
                <field name="bsd_pricelist_id"/>
            </xpath>
        </field>
    </record>
</odoo>